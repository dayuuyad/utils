pipeline {
    agent any

    stages {
        stage('SonarQube Analysis') {
            steps {

                script {
                    scannerHome = tool 'sonarqubeScanner'
                }

                withSonarQubeEnv('sonarqube') {
                    powershell '${scannerHome}/bin/sonar-scanner.bat'
                }


            }
        }

    }
    }
}